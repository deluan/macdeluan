	name = Deluan
	email = deluan@thoughtworks.com

[push]
	default = simple

[alias]
	st = status
	co = checkout
	ci = commit
	pr = pull --rebase
	lg = log --pretty=format:'%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]' --decorate
	ll = git config --global alias.ll "log --pretty=format:'%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]' --decorate --numstat"